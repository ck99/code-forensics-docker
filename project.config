[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/ck99
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/ck99
	label-Verified = -1..+1 group user/ck99
	push = group user/ck99
	read = group user/ck99
	rebase = group user/ck99
	removeReviewer = group user/ck99
	submit = group user/ck99
[access "refs/heads/*"]
	create = group user/ck99
	push = +force group user/ck99
	pushMerge = group user/ck99
	read = group user/ck99
[access "refs/tags/*"]
	push = group user/ck99
	push = +force group user/ck99
